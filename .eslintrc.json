{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": [
            "off",
            "never"
        ],
        "comma-dangle": [
            "off",
            "never"
        ],
        "arrow-parens": [
            "off",
            "never"
        ]
    }
}
